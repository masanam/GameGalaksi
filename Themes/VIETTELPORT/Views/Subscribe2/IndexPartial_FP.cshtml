        <a class="buttons" href="@Url.Action("Index", "Home")" style="width:45%;float:left;display:block;">
            @Html.PortalText("NoButton")
        </a>
        <a class="buttons yesButton" href="@Url.Action("Subscribe", "Subscribe")" style="display: inline;">
            @Html.PortalText("YesButton")
        </a>
