<div id="top" align="center" style="padding: 1%; text-align: center; background-color: #32CCFE;
    font-weight: bold; font-size: 1em;">@Html.PortalText("SubscribeJoinText")
</div>
<div class="text-about-price element" style="padding: 0 2%; font-weight: bold; color: rgb(243, 115, 153);">
    @Html.PortalText("SubscribePriceText")
</div>
<div id="benefits" class="element" style="padding: 0 2%; font-size: 12px;">
    @Html.PortalText("SubscribeBenefit")
</div>
